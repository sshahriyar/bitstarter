	<div id="content" class="txt-middle">
	  <div class="container">
              <div class="row">
		  <div class="span12">
                      <h2>Your Account Information</h2><br>
                      <p>This site is a crowd funder for Debug007. Complete functionality will soon be available. 
                          Thank you for becoming a member.</p>
                        
		    <hr class="dashed">				
		  </div>  
		</div>  
              
	    <div class="row"> 
                
		  <div class="span6">
		    <div class="well well-form txt-lefty">
                    <h3>Update Your Account Settings</h3>
                        
		     <form method="post" id="account-form">
                     
                         <input type="hidden" id="userId" value="<%= uid %>" name="userid">
		        <div class="control-group">
		          <div id="fname-cg" class="controls">
					<input type="text" id="fname-tf" maxlength="150" name="firstName" value="<%= firstName %>"placeholder="First Name" class="span3" required>
				  </div>
			    </div>
		        <div class="control-group">
		          <div id="lname-cg" class="controls">
					<input type="text" id="lname-tf" maxlength="150" name="lastName" value= "<%= lastName %>"placeholder="Last Name" class="span3" required>
				  </div>
			    </div>
		        <div id="user-cg" class="control-group">
		          <div class="controls">
					<input type="email" id="user-tf" maxlength="255" name="user" placeholder="Email (Username)" value=<%= email %> class="span3" required>
				  </div>
			    </div>
		        <div id="pass-cg" class="control-group">
		          <div class="controls">
					<input type="password" id="pass-tf" minlength="6" maxlength="25" name="pass" placeholder="Password" class="span3" required>
				  </div>
			    </div>
		        <div class="control-group">
		          <div class="controls">
					<input type="password" id="pass-again" minlength="6" maxlength="25" name="passagain" placeholder="Confirm Password" class="span3" required data-validation-matches-match="pass" data-validation-matches-message="Must match password entered above">
				  </div>
			    </div>
                          <div id="message-cg" class="control-group" >
		          <div class="controls">
					<textarea id="message-tf" name="message" placeholder="Message: Organization, URL or any thing else" 
                                                  class="span3" maxlength="500"  type="text" rows="5"   > <%= message %></textarea>
				  </div>
			    </div>
                <div class="form-actions">
			      
			      <button class="btn btn-primary" type="submit" id="account-form-btn2">Update</button>
                </div> 
              </form>
		      <div class="form-extra">
			  <!--  <button class="btn btn-info"><i class="glyphicons-twitter"></i> Login with Twitter</button>
			    <button class="btn btn-danger"><i class="glyphicons-google_plus"></i> Login with Google</button> -->
			  </div> 			  
            </div>
       </div>
             
                
            <div class="span5">
                <h3> You can contribute by clicking below </h3>
             <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
                   <input type="hidden" name="cmd" value="_s-xclick">
                   <input type="hidden" name="hosted_button_id" value="MFA9QYTBNBRBS">
                   <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif"  name="submit" alt="Use PayPal!">
                   <img alt=""  src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
                </form>
            </div>
   <hr class="dashed">	
                
            
        <div class="span5">
             <h2>Your Donation Details</h2>
                <table class="table table-striped ">
                  <thead>
                    <th>Transaction ID</th>
                    <th>Amount</th>
                    <th>Transaction time</th>
                    <th>Transaction Type</th>
                  </thead>
                  <tbody>
                    <% for(var i = 0; i < orders.length; i++) {%>
                       <tr>
                         <td><%= orders[i].transactionId %></td>
                         <td><%= orders[i].amount %></td>
                         <td><%= orders[i].transactionTime %></td>
                         <td><%= orders[i].transactionType %></td>
                       </tr>
                    <% } %>
                  </tbody>             
             </table>

        </div>
      
<!--row -->
  </div>  
 </div>  
