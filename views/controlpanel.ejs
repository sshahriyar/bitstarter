<!-- display form errors in a custom modal window //-->
  <div class="modal-form-errors modal hide fade">
    <div class="modal-header">
      <button data-dismiss="modal" class="close">x</button>
      <h3>Whoops!</h3>
    </div>
    <div class="modal-body">
      <p class="subheading">It looks like there was a problem.</p>
      <ul></ul>
    </div>
    <div class="modal-footer">
      <button data-dismiss="modal" class="btn btn-danger">OK</button>
    </div>
  </div>

  <div class="modal-alert modal hide fade">
    <div class="modal-header">
      <button data-dismiss="modal" class="close">x</button>
      <h3></h3>
    </div>
    <div class="modal-body">
      <p></p>
    </div>
    <div class="modal-footer">
      <button data-dismiss="modal" id="ok" class="btn btn-danger">OK</button>
    </div>
  </div>

 <div class="modal-confirm modal hide fade">
    <div class="modal-header">
      <button data-dismiss="modal" class="close">x</button>
      <h3></h3>
    </div>
    <div class="modal-body">
      <p></p>
    </div>
    <div class="modal-footer">
      <button data-dismiss="modal" class="cancel btn">Cancel</button>
      <button class="submit btn">Ok</button>
    </div>
  </div>


