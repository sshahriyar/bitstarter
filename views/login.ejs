	<div id="content">
	  <div class="container">
	    <div class="row">
		  <div class="span12">
		    <div class="well well-form txt-lefty">
              <h3>Sign In</h3>
		  <form id="login-form" method="post">
		        <div class="control-group">
		          <div class="controls">
			        <div class="input-prepend">
			          <span class="add-on"><i class="icon-user"></i></span>
					  <input type="email" name="user"  maxlength="255" id="user-tf" placeholder="Username (your email)" class="span3" required>
			        </div>
				  </div>
			    </div>
			    <div class="control-group"> 
		          <div class="controls">
			        <div class="input-prepend">
			          <span class="add-on"><i class="icon-lock"></i></span>
					  <input type="password" name="pass"  id="pass-tf" minlength="6" maxlength="25" placeholder="Password" class="span3" required>
			        </div>
				  </div>			
		        </div>
                <div class="form-actions">
			      <label class="checkbox" id="remember-me"  name="remember-me">
                                 <input type="checkbox" name="remember">       Remember Me
                              </label>
			      <button class="btn btn-primary" type="submit">Sign In</button>
                </div> 
              </form>
	   <div class="form-extra" id="login-form-extra">
              <!-- <div class="btm-links">  -->
		
                <a href="#" id="forgot-password"> <button  class="btn btn-info">Forgot Your Password? </button> </a>
			   <a href="/signup" id="create-account"> <button  class="btn btn-danger"> Create An Account</button>	</a>

                      <!--  <button class="btn btn-info"><i class="glyphicons-twitter"></i> Login with Twitter</button>
